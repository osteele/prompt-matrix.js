{
  "name": "prompt-matrix",
  "version": "0.0.1",
  "description": "A utility to expand prompt matrices.",
  "main": "dist/expandPrompt.js",
  "types": "dist/expandPrompt.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest src",
    "test:watch": "jest --watch src",
    "format": "prettier --write src",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run lint && npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osteele/prompt-matrix.js.git"
  },
  "keywords": [
    "prompt",
    "matrix",
    "utility"
  ],
  "author": "Oliver Steele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/osteele/prompt-matrix.js/issues"
  },
  "readme": "README.md",
  "homepage": "https://github.com/osteele/prompt-matrix.js#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "prettier": "^2.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.4.4"
  }
}
